{
	"name": "Copy_Lego_Themes_ADLS_to_SQL",
	"properties": {
		"activities": [
			{
				"name": "Copiar Datos ADLS a SQL",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "lego//themes.parquet"
					},
					{
						"name": "Destination",
						"value": "lego.themes"
					}
				],
				"typeProperties": {
					"source": {
						"type": "ParquetSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": false,
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"enableSkipIncompatibleRow": true,
					"logSettings": {
						"enableCopyActivityLog": true,
						"copyActivityLogSettings": {
							"logLevel": "Warning",
							"enableReliableLogging": false
						},
						"logLocationSettings": {
							"linkedServiceName": {
								"referenceName": "AzureDataLakeStorageTutorial",
								"type": "LinkedServiceReference"
							}
						}
					},
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "TemeID",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "TemeID",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "ThemeName",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "ThemeName",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "Parent_ThemeID",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "Parent_ThemeID",
									"type": "String",
									"physicalType": "String"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ADLS_lego_themes",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ASQL_lego_themes",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}