{
	"name": "ADLS_to_ASQL_Param",
	"properties": {
		"activities": [
			{
				"name": "Copy ADLS to ASQL Param",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "lego//@{'sets.csv'}"
					},
					{
						"name": "Destination",
						"value": "lego.@{'lego.sets'}"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "ADLS_Lego",
						"type": "DatasetReference",
						"parameters": {
							"FileNameADLS": "sets.csv"
						}
					}
				]
			}
		],
		"annotations": []
	}
}